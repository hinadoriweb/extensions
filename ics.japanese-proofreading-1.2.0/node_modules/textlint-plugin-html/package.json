    "@textlint/module-interop": "^12.1.0",
    "glob": "^7.1.1",
    "mocha": "^9.1.3",
    "textlint": "^12.1.0",
    "textlint-rule-no-todo": "^2.0.0"
  },
  "dependencies": {
    "hast": "0.0.2",
    "structured-source": "^3.0.2",
